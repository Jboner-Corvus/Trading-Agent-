{
  "name": "deamon-dev-trading-dashboard",
  "version": "1.0.0",
  "description": "Real-time trading dashboard for Deamon Dev AI",
  "main": "run.ts",
  "scripts": {
    "start": "ts-node run.ts start",
    "server": "ts-node run.ts start",
    "dev": "nodemon --exec ts-node run.ts start",
    "stop": "ts-node run.ts stop",
    "clean": "ts-node run.ts stop",
    "restart": "ts-node run.ts restart",
    "test": "ts-node run.ts test",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "node lint-format.js",
    "lint-fix": "node lint-format.js --fix",
    "format": "npx prettier --write \"**/*.{js,ts,json}\"",
    "format-check": "npx prettier --check \"**/*.{js,ts,json}\"",
    "eslint": "npx eslint \"**/*.{js,ts}\"",
    "eslint-fix": "npx eslint --fix \"**/*.{js,ts}\"",
    "lint-py": "python lint-format-py.py",
    "lint-py-fix": "python lint-format-py.py --fix",
    "format-py": "black src/ --line-length=88 && isort src/ --profile=black",
    "format-py-check": "black --check src/ --line-length=88 && isort --check-only src/ --profile=black"
  },
  "dependencies": {
    "@types/joi": "^17.2.2",
    "axios": "^1.13.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "joi": "^18.0.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "trading",
    "dashboard",
    "real-time",
    "crypto"
  ],
  "author": "Deamon Dev AI"
}
